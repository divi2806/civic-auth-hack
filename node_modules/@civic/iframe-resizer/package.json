{
  "name": "@civic/iframe-resizer",
  "version": "0.1.2",
  "description": "Custom iframe resizer for Civic Auth",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./child": {
      "types": "./dist/child.d.ts",
      "import": "./dist/child.mjs",
      "require": "./dist/child.js"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "18.3.11",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.0.0",
    "typescript": "^5.6.2",
    "@repo/eslint-config": "0.0.0",
    "@repo/typescript-config": "0.0.0"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "private": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts*\" --max-warnings 0",
    "lint:fix": "pnpm lint --fix"
  }
}